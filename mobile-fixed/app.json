{
  "expo": {
    "name": "Pic2Nav",
    "slug": "pic2nav",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ssabiroad.pic2nav",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to camera to take photos for location analysis.",
        "NSPhotoLibraryUsageDescription": "This app needs access to photo library to select images for location analysis.",
        "NSLocationWhenInUseUsageDescription": "This app needs location access to show your current position on the map.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "googleMapsApiKey": "AIzaSyCZBx_VHFbaGx-8Y5V81rkL2U-lITY4yhY"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ssabiroad.pic2nav",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCZBx_VHFbaGx-8Y5V81rkL2U-lITY4yhY"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Pic2Nav to access your photos for location analysis."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Pic2Nav to use your location to show your position on the map."
        }
      ],
      "expo-secure-store"
    ],
    "extra": {
      "GOOGLE_PLACES_API_KEY": "AIzaSyCZBx_VHFbaGx-8Y5V81rkL2U-lITY4yhY",
      "GOOGLE_MAPS_API_KEY": "AIzaSyCZBx_VHFbaGx-8Y5V81rkL2U-lITY4yhY",
      "eas": {
        "projectId": "34bf28af-9961-40f3-8789-b5ba52b54f96"
      }
    },
    "owner": "davitacols"
  }
}
